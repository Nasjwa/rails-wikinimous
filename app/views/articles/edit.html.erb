<h1>Edit Article</h1>

<%= form_with model: @article do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :content %><br>
    <%= f.text_area :content %>
  </p>
  <p><%= f.submit %></p>
<% end %>

<p><%= link_to "Back", article_path(@article) %></p>
